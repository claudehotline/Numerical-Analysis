    "def triangle_wave3(x, c, c0, hc):\n",
    "    x = x - x.astype(np.int)\n",
    "    return np.piecewise(x,\n",
    "        [x >= c, x < c0],\n",
    "        [0,\n",
    "        lambda x: x / c0 * hc,\n",
    "        lambda x: (c - x) / (c - c0) * hc])\n",
    "\n",
    "x = np.linspace(0, 2, 10000)\n",
    "y3 = triangle_wave3(x, 0.6, 0.4, 1.0)\n",
    "print y3"
